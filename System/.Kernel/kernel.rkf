else
local RunKernelFunction = shell.resolve( tArgs[1] )
if RunKernelFunction ~= nul then
  if RunKernelFunction == "restart" then
    restart()
  elseif RunKernelFunction == "CheckReg" then
    CheckReg() --Checks for reg changes and global changes and applies them
  end
else
  return nul
end
end
